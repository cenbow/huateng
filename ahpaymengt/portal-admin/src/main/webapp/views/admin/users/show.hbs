<h1>User {{user.id}}</h1>


<dl>
    <dt>Email:</dt>
    <dd>{{user.email}}</dd>

    <dt>Name:</dt>
    <dd>{{user.name}}</dd>

    <dt>Type:</dt>
    <dd>{{user.type}}</dd>

    <dt>Status:</dt>
    <dd>{{user.status}}</dd>

    <dt>realName:</dt>
    <dd>{{userProfile.realName}}</dd>

    <dt>description:</dt>
    <dd>{{userProfile.description}}</dd>
</dl>

<div class="form-actions">
    <a href="/users" class="btn"><span>Back</span></a>
    <a href="/users/{{id}}/edit" class="btn"><span>Edit</span></a>
</div>
{{/partial}}
{{> admin/layout}}

<div class="app-modal comp-panel" id="js-add-component-panel">
    <b class="menu-icon"></b>

    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="go-back" id="js-component-go-back">返回添加组件</h4>
    </div>
    <div class="modal-body scrollable none">
        <p class="app-introduction">你可以在这里选择要添加的组件</p>
        <ul class="app-list unstyled" id="js-comps-list">
            {{#each data}}
            <li class="{{this.last}}" data-comp-id="{{this.id}}"><span class="{{this.icon}}"></span>{{this.name}}</li>
            {{/each}}
        </ul>
    </div>
</div>

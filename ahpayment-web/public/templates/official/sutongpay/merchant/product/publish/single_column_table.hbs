<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>{{entries.0.key}}</th>
      <th>数量</th>
      <th>价格</th>
      <th>外部商户Id</th>
    </tr>
  </thead>
  <tbody>
    <tr>
    {{#each entries}}
      <td class="column" data-id="{{id}}" data-name="{{value}}">{{value}}</td>
      <td><input class="input-small" type="text" name="stock" data-real-id="{{id}}" /></td>
      <td><input class="input-small" type="text" name="price" data-real-id="{{id}}" /></td>
      <td><input class="input-small" type="text" name="outerId" data-real-id="{{id}}" /></td>
      </tr>
    </tr>
    {{/each}}
  </tbody>
</table>

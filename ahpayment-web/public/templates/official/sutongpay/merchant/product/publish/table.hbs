<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>{{entries.0.key}}</th>
      <th>{{entries.0.values.0.value.key}}</th>
      <th>数量</th>
      <th>价格</th>
      <th>外部商户Id</th>
    </tr>
  </thead>
  <tbody>
    <tr>
    {{#each entries}}
      <td class="main-column" rowspan="{{values.length}}" data-id="{{id}}" data-name="{{value}}">{{value}}</td>
      {{#each values}}
      <td class="column" data-id="{{value.id}}" data-name="{{value.value}}">{{value.value}}</td>
      <td><input class="input-small" type="text" name="stock" data-real-id="{{realId}}" /></td>
      <td><input class="input-small" type="text" name="price" data-real-id="{{realId}}" /></td>
      <td><input class="input-small" type="text" name="outerId" data-real-id="{{realId}}" /></td>
      </tr>
      {{/each}}
    </tr>
    {{/each}}
  </tbody>
</table>
